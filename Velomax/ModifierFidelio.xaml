<Window x:Class="Velomax.ModifierFidelio"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Velomax"
        mc:Ignorable="d"
        Title="Modifier programme Fidélio" Height="150" Width="400" Background="#FFD9D9D9" WindowStartupLocation="CenterScreen" ResizeMode="CanMinimize">
    <Canvas>
        <Button x:Name="bEnregistrer" Content="OK" HorizontalAlignment="Left" Height="34" VerticalAlignment="Top" Width="90" FontSize="24" FontFamily="Corbel Light" Background="#FF299ACF" BorderBrush="#FFC33F23" Cursor="Hand" Canvas.Left="260" Canvas.Top="44" Click="bOk">
            <Button.Effect>
                <DropShadowEffect Color="#FFC33F23"/>
            </Button.Effect>
        </Button>
        <ComboBox x:Name="cbFidelio" Canvas.Left="41" Canvas.Top="44" Width="195" FontSize="18" Height="34"/>

    </Canvas>
</Window>
